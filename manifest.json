{
  "name": "Timezonr",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Get the location and time zone from a US/Canada area code",
  "browser_action": {
    "default_icon": "clock.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "http://www.allareacodes.com/"
  ]
}